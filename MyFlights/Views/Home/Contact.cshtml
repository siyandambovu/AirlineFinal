<body>

    <section id="content" class="content-subpages">


        <div>
            <div class="col-md-12">
                <div class="col-md-12">


                    <h2>Feedback</h2>
                    
                    <form id="form" method="post" action="#">
                    @using (Html.BeginForm("Contact", "Contacts", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
                    {
                        @Html.AntiForgeryToken()
                        <fieldset>
                            <label >
                                <strong>Subject:</strong>
                                <input type="text" value="" name="subject">
                            </label>
                            <br />
                            <br />
                            <label>
                                <strong>Email:</strong>
                                <input type="text" value="" name="email">
                            </label>
                            <br />
                            <br />
                            <label>
                                <strong>Message:</strong>
                                <textarea row="5" cols="20" name="message"></textarea>
                            </label>
                            <br />
                            @*<div class="btns"><a href="~/#" class="link">Clear</a><a href="~/#" class="link">Send</a></div>*@
                            <button type="submit" id="submit" class="btn-group">Send</button>
                        </fieldset>
                        }
                    </form>
                </div>
                <div class="col-2">
                    <h2 class="p2">Office Times</h2>
                    <p>
                        Monday - Friday: 7:30 am - 18:00<br>
                        Saturday: 7:30 am - 13:00<br>
                        
                    </p>
